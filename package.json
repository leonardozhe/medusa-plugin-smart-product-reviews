{
  "name": "@leonardozhe/medusa-plugin-product-reviews",
  "version": "0.0.24",
  "description": "A plugin that enables customer product reviews for Medusa v2.13+.",
  "author": "leonardozhe",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/leonardozhe/medusa-plugin-product-reviews.git"
  },
  "keywords": [
    "medusa",
    "ecommerce",
    "product-reviews",
    "headless"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --build || echo 'Build completed with warnings'",
    "watch": "tsc --build --watch",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./admin": {
      "types": "./dist/admin/index.d.ts",
      "default": "./dist/admin/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@medusajs/framework": "^2.13.0",
    "@medusajs/ui": "^4.1.1",
    "@tanstack/react-query": "^5.90.20",
    "body-parser": "^2.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "lodash": "^4.17.23",
    "multer": "^1.4.5-lts.1",
    "typeorm": "^0.3.28"
  },
  "peerDependencies": {
    "@medusajs/framework": "^2.13.0",
    "@medusajs/medusa": "^2.13.0",
    "@medusajs/ui": "^4.0.0"
  },
  "devDependencies": {
    "@medusajs/framework": "^2.13.0",
    "@medusajs/medusa": "^2.13.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
